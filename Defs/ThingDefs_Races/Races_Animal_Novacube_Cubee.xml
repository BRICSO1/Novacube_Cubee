<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!-- Caractéristiques générales de la Cubee -->
	<ThingDef ParentName="BaseInsect">
    <defName>Novacube_Cubee</defName>
    <label>Cubee</label>
    <description>Une imposante abeille à la tête carrée issue de la planète Novacube. De nature paisible et fragile, méfiez-vous tout de même de son puissant dard. Elle peut être élevée pour son miel au goût incomparable.</description>
    <statBases>
      <MoveSpeed>3.75</MoveSpeed>
      <MarketValue>300</MarketValue>
	  <ComfyTemperatureMin>10</ComfyTemperatureMin>
      <ComfyTemperatureMax>50</ComfyTemperatureMax>
    </statBases>
    <comps>
      <li Class="CompProperties_EggLayer">
        <eggFertilizedDef>EggCubeeFertilized</eggFertilizedDef>
        <eggFertilizationCountMax>5</eggFertilizationCountMax>
        <eggLayIntervalDays>20</eggLayIntervalDays>
        <eggProgressUnfertilizedMax>0.5</eggProgressUnfertilizedMax>
        <eggCountRange>1~5</eggCountRange>
      </li>
      <li Class="CompProperties_Milkable">
        <milkDef>InsectHoney</milkDef>
        <milkIntervalDays>3</milkIntervalDays>
        <milkAmount>10</milkAmount>
        <milkFemaleOnly>false</milkFemaleOnly>
      </li>
    </comps>
    <uiIconScale>2</uiIconScale>
    <tools>
      <li>
        <label>mandibles</label>
        <capacities>
          <li>Bite</li>
        </capacities>
        <power>5</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>Mouth</linkedBodyPartsGroup>
      </li>
      <li>
        <label>head</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>4</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
        <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
        <chanceFactor>0.1</chanceFactor>
      </li>
      <li>
        <label>Stinger</label>
        <capacities>
          <li>Stab</li>
          <li>ToxicBite</li>
        </capacities>
        <power>25</power>
        <cooldownTime>100</cooldownTime>
        <linkedBodyPartsGroup>Bottom</linkedBodyPartsGroup>
        <ensureLinkedBodyPartsGroupAlwaysUsable>false</ensureLinkedBodyPartsGroupAlwaysUsable>
        <chanceFactor>1</chanceFactor>
      </li>
    </tools>
    <race>
      <body>BeeLike</body>
      <baseBodySize>0.2</baseBodySize>
      <baseHungerRate>0.10</baseHungerRate>
      <baseHealthScale>0.4</baseHealthScale>
      <foodType>VegetarianRoughAnimal</foodType>
      <packAnimal>true</packAnimal>
      <trainability>None</trainability>
      <manhunterOnDamageChance>1</manhunterOnDamageChance>
      <manhunterOnTameFailChance>0.1</manhunterOnTameFailChance>
      <wildness>2</wildness>
      <roamMtbDays>7</roamMtbDays>
      <mateMtbHours>8</mateMtbHours>
      <lifeExpectancy>6</lifeExpectancy>
      <lifeStageAges>
        <li>
          <def>EusocialInsectLarva</def>
          <minAge>0</minAge>
        </li>
        <li>
          <def>EusocialInsectJuvenile</def>
          <minAge>0.03</minAge>
        </li>
        <li>
          <def>EusocialInsectAdult</def>
          <minAge>0.4</minAge>
          <soundWounded>Pawn_Cubee_Wounded</soundWounded>
          <soundDeath>Pawn_Cubee_Death</soundDeath>
          <soundCall>Pawn_Cubee_Call</soundCall>
          <soundAngry>Pawn_Cubee_Angry</soundAngry>
        </li>
      </lifeStageAges>
      <soundMeleeHitPawn>Pawn_Melee_SmallScratch_HitPawn</soundMeleeHitPawn>
      <soundMeleeHitBuilding>Pawn_Melee_SmallScratch_HitBuilding</soundMeleeHitBuilding>
      <soundMeleeMiss>Pawn_Melee_SmallScratch_Miss</soundMeleeMiss>
      <headPosPerRotation>
        <li>(0.0, 0, 0.12)</li>
        <li>(0.07, 0, -0.05)</li>
        <li>(0.0, 0, -0.08)</li>
        <li>(-0.07, 0, -0.05)</li>
      </headPosPerRotation>
    </race>
    <tradeTags>
      <li>AnimalInsect</li>
      <li>AnimalUncommon</li>
      <li>AnimalExotic</li>
    </tradeTags>
  </ThingDef>
  
